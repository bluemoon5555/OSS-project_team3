#include<stdio.h>

int add(int n,int m){
  return n + m;
}//더하기

int minus(int n,int m)
{
	return(n - m);
}//빼기

int multi(int n,int m){
	return n * m;
  
}//곱하기

int divi(int n,int m){
    if (m == 0) {
        printf("error: division by zero!/n");
        return 0;
    }
    return n / m;    
}//나누기

int remain(int n,int m){
if(n<m){
int t=n;
n=m;
m=t;
}
return n%m;  
}//나머지 연산

int main(){
int a,b;
printf("Enter two number>>");
scanf_s("%d%d",&a,&b);

printf("%d + %d=%d\n",a,b,add(a,b));

printf("%d - %d=%d\n",a,b,minus(a,b));

printf("%d * %d=%d\n",a,b,multi(a,b));

printf("%d / %d=%d\n",a,b,divi(a,b));

printf("%d %% %d=%d\n",a,b,remain(a,b));

return 0;

}
